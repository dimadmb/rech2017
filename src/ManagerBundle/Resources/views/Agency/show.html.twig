{% extends 'basemanager.html.twig' %}

{% block body %}
    <h1>Agency</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ agency.id }}</td>
            </tr>
            <tr>
                <th>Bankname</th>
                <td>{{ agency.bankName }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ agency.name }}</td>
            </tr>
            <tr>
                <th>Rs</th>
                <td>{{ agency.rs }}</td>
            </tr>
            <tr>
                <th>Ks</th>
                <td>{{ agency.ks }}</td>
            </tr>
            <tr>
                <th>Bik</th>
                <td>{{ agency.bik }}</td>
            </tr>
            <tr>
                <th>Inn</th>
                <td>{{ agency.inn }}</td>
            </tr>
            <tr>
                <th>Kpp</th>
                <td>{{ agency.kpp }}</td>
            </tr>
            <tr>
                <th>Uraddress</th>
                <td>{{ agency.urAddress }}</td>
            </tr>
            <tr>
                <th>Faktaddress</th>
                <td>{{ agency.faktAddress }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ agency.phone }}</td>
            </tr>
            <tr>
                <th>Fee</th>
                <td>{{ agency.fee }}</td>
            </tr>
            <tr>
                <th>Numdog</th>
                <td>{{ agency.numDog }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{% if agency.created %}{{ agency.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{% if agency.updated %}{{ agency.updated|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Shortname</th>
                <td>{{ agency.shortName }}</td>
            </tr>
            <tr>
                <th>Auth</th>
                <td>{{ agency.auth }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('manager_agency_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('manager_agency_edit', { 'id': agency.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
